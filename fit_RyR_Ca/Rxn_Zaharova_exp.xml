<ReactionScheme>
  <Specie name="Ca" id="Ca" kdiff="100" kdiffunit="mu2/s"/>
  <Specie name="RyR" id="RyR" kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="CaRyR" id="CaRyR" kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca3RyR4" id="Ca3RyR4" kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca3RyR4O" id="Ca3RyR4O" kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca4RyR4" id="Ca4RyR4" kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca4RyR4O" id="Ca4RyR4O" kdiff="0" kdiffunit="mu2/s"/>

<Reaction name="RyRe" id="RyRe">
    <Reactant specieID="RyR"/>
    <Reactant specieID="Ca"/>
    <Product specieID="CaRyR"/>
    <forwardRate>712e-6</forwardRate>
    <reverseRate>3</reverseRate>
 </Reaction>

  <Reaction name="RyRa" id="RyRa">
    <Reactant specieID="CaRyR" power="3"/>
    <Reactant specieID="RyR"/>
    <Product specieID="Ca3RyR4"/>
    <forwardRate>5e-6</forwardRate>
    <reverseRate>9600e-4</reverseRate>
 </Reaction>
   
<Reaction name="RyRb" id="RyRb">
    <Reactant specieID="Ca3RyR4"/>
    <Product specieID="Ca3RyR4O"/>
    <forwardRate>10</forwardRate>
    <reverseRate>0.4</reverseRate>
  </Reaction>


  <Reaction name="RyRa" id="RyRa">
    <Reactant specieID="CaRyR" power="4"/>
    <Product specieID="Ca4RyR4"/>
    <forwardRate>5e-6</forwardRate>
    <reverseRate>9600e-4</reverseRate>
 </Reaction>
   
<Reaction name="RyRb" id="RyRb">
    <Reactant specieID="Ca4RyR4"/>
    <Product specieID="Ca4RyR4O"/>
    <forwardRate>10</forwardRate>
    <reverseRate>0.4</reverseRate>
  </Reaction>

  
  <Specie name="DMN" id="DMN" kdiff="150" kdiffunit="mu2/s"/>
  <Specie name="DMN_buf" id="DMN_buf" kdiff="150" kdiffunit="mu2/s"/>
  <Specie name="CaDMN_buf" id="CaDMN_buf" kdiff="150" kdiffunit="mu2/s"/>
  <Specie name="CaDMN" id="CaDMN" kdiff="150" kdiffunit="mu2/s"/>
  <Reaction name="DMNinj" id="DMNinj">
    <Reactant specieID="CaDMN"/>
    <Product specieID="Ca"/>
    <Product specieID="DMN"/>
    <forwardRate>10</forwardRate>
  </Reaction>
  <Reaction name="DMNtrans" id="DMNtrans">
    <Reactant specieID="DMN"/>
    <Product specieID="DMN_buf"/>
    <forwardRate>1</forwardRate>
  </Reaction>
  <!--Faas GC, Karacs K, Vergara JL, Mody I. Kinetic properties of DM-nitrophen binding to calcium and magnesium. Biophys J. 2005 Jun;88(6):4421-33. doi: 10.1529/biophysj.104.057745. Epub 2005 Mar 18. PMID: 15778435; PMCID: PMC1305669.-->
   <Reaction name="DMNtrans" id="DMNtrans">
     <Reactant specieID="DMN_buf"/>
    <Reactant specieID="Ca"/>
    <Product specieID="CaDMN_buf"/>
    <forwardRate>2.6e-4</forwardRate>
    <reverseRate>7.52e-3</reverseRate>
   </Reaction>

</ReactionScheme>

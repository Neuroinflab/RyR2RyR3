<?xml version="1.0"?>
<ReactionScheme>
  <Specie name="Ca" id="Ca" kdiff="100" kdiffunit="mu2/s" />
  <!-- <Specie name="CaER" id="CaER" kdiff="10." kdiffunit="mu2/s"/> -->
  <Specie name="CaM" id="CaM" kdiff="4." kdiffunit="mu2/s"/>
  <Specie name="CaMCa2C" id="CaMCa2C" kdiff="4." kdiffunit="mu2/s"/>
  <Specie name="CaMCa4" id="CaMCa4" kdiff="4." kdiffunit="mu2/s"/>
  <Specie name="RyR"            id="RyR"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="CaRyR"            id="CaRyR"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyRCaM"            id="RyRCaM"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyRCaMCa2C"            id="RyRCaMCa2C"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyRCaMCa4"            id="RyRCaMCa4"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="CaRyRCaM"            id="CaRyRCaM"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="CaRyRCaMCa2C"            id="CaRyRCaMCa2C"      kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="CaRyRCaMCa4"            id="CaRyRCaMCa4"       kdiff="0" kdiffunit="mu2/s"/>

  <!--1) CaM  + 2Ca
      <-> CaMCa2 -->
  <!--This is the C site-->
  <Reaction name="CaMC_bind" id="CaMC_bind">
    <Reactant specieID="CaM" />
    <Reactant specieID="Ca" n="2" />
    <Product specieID="CaMCa2C" />
    <forwardRate>6e-6</forwardRate>
    <reverseRate>9.1e-3</reverseRate>
    <Q10>0.2</Q10>
  </Reaction>

  <Reaction name="CaMCa2C_bind" id="CaMCa2C_bind">
    <Reactant specieID="CaMCa2C" />
    <Reactant specieID="Ca" n="2" />
    <Product specieID="CaMCa4" />
    <forwardRate>0.1e-3</forwardRate>
    <reverseRate>1000e-3</reverseRate>
    <Q10>0.2</Q10>
  </Reaction>

  <Reaction name = "RyRe" id="RyRe">
    <Reactant specieID="RyR"/>
    <Reactant specieID="Ca"/>
    <Product specieID="CaRyR"/>
    <forwardRate>712e-6</forwardRate>
    <reverseRate>3</reverseRate>
 </Reaction>
  
  <Reaction name = "RyRCaMe1" id="RyRCaMe1">
    <Reactant specieID="RyRCaM"/>
    <Reactant specieID="Ca"/>
    <Product specieID="CaRyRCaM"/>
    <forwardRate>712e-6</forwardRate>
    <reverseRate>3</reverseRate>
  </Reaction>

  <Reaction name = "RyRCaMe2" id="RyRCaMe2">
    <Reactant specieID="RyRCaMCa2C"/>
    <Reactant specieID="Ca"/>
    <Product specieID="CaRyRCaMCa2C"/>
    <forwardRate>712e-6</forwardRate>
    <reverseRate>3</reverseRate>
  </Reaction>

  <Reaction name = "RyRCaMe3" id="RyRCaMe3">
    <Reactant specieID="RyRCaMCa4"/>
    <Reactant specieID="Ca"/>
    <Product specieID="CaRyRCaMCa4"/>
    <forwardRate>712e-6</forwardRate>
    <reverseRate>3</reverseRate>
  </Reaction>

  <!-- calculated from Balshaw-->
  <!-- also in lit, -->
   <Reaction name = "RyRCaMf1" id="RyRCaMf1">
     <Reactant specieID="RyR"/>
     <Reactant specieID="CaM"/>
     <Product specieID="RyRCaM"/>
     <forwardRate>2.4e-7</forwardRate>
     <reverseRate>1.73e-5</reverseRate>
   </Reaction>
   <!-- calculated from Balshaw
	kon 3.15e-7 nM-1ms-1 (koff 3.67e-8) ( 
   -->
   <Reaction name = "RyRCaMf2" id="RyRCaMf2">
     <Reactant specieID="RyR"/>
     <Reactant specieID="CaMCa2C"/>
     <Product specieID="RyRCaMCa2C"/>
     
     <forwardRate>3.15e-7</forwardRate>
     <reverseRate>3.67e-6</reverseRate>
   </Reaction>
   <!-- https://doi.org/10.1161/CIRCRESAHA.114.302857-->
   <Reaction name = "RyRCaMf3" id="RyRCaMf3">
     <Reactant specieID="RyR"/>
     <Reactant specieID="CaMCa4"/>
     <Product specieID="RyRCaMCa4"/>
     
     <forwardRate>3.66e-7</forwardRate>
     <reverseRate>1.28e-6</reverseRate>
   </Reaction>
     <!-- calculated from Balshaw-->
   <Reaction name = "CaRyRCaMf1" id="CaRyRCaMf1">
     <Reactant specieID="CaRyR"/>
     <Reactant specieID="CaM"/>
     <Product specieID="CaRyRCaM"/>
     <forwardRate>2.4e-7</forwardRate>
     <reverseRate>1.73e-5</reverseRate>
   </Reaction>
   <!-- calculated from Balshaw--> 
   <Reaction name = "CaRyRCaMf2" id="CaRyRCaMf2">
     <Reactant specieID="CaRyR"/>
     <Reactant specieID="CaMCa2C"/>
     <Product specieID="CaRyRCaMCa2C"/>

    <forwardRate>3.15e-7</forwardRate>
    <reverseRate>3.67e-6</reverseRate>
   </Reaction>
   <!-- again use Balshaw-->
   <Reaction name = "CaRyRCaMf3" id="CaRyRCaMf3">
     <Reactant specieID="CaRyR"/>
     <Reactant specieID="CaMCa4"/>
     <Product specieID="CaRyRCaMCa4"/>
     
     <forwardRate>3.66e-7</forwardRate>
     <reverseRate>1.28e-6</reverseRate>
   
   </Reaction>
   <!-- increase CaM affinity for Ca by a factor of 10-->
   <Reaction name="RyRCaMC_bind" id="RyRCaMC_bind">

     <Reactant specieID="RyRCaM" />
     <Reactant specieID="Ca" n="2" />
     <Product specieID="RyRCaMCa2C" />
     <forwardRate>6e-5</forwardRate>
     <reverseRate>9.1e-3</reverseRate>
     <Q10>0.2</Q10>
   </Reaction>
   <!-- increase CaM affinity for Ca by a factor of 10-->
   <Reaction name="RyRCaMCa2C_bind" id="RyRCaMCa2C_bind">
     
     <Reactant specieID="RyRCaMCa2C" />
     <Reactant specieID="Ca" n="2" />
     <Product specieID="RyRCaMCa4" />
     <forwardRate>0.1e-2</forwardRate>
     <reverseRate>1000e-3</reverseRate>
     <Q10>0.2</Q10>
   </Reaction>
   <!-- increase CaM affinity for Ca by a factor of 10-->
   <Reaction name="CaRyRCaMC_bind" id="CaRyRCaMC_bind">
     
     <Reactant specieID="CaRyRCaM" />
     <Reactant specieID="Ca" n="2" />
     <Product specieID="CaRyRCaMCa2C" />
     <forwardRate>6e-5</forwardRate>
     <reverseRate>9.1e-3</reverseRate>
     <Q10>0.2</Q10>
   </Reaction>
   <!-- increase CaM affinity for Ca by a factor of 10-->
   <Reaction name="CaRyRCaMCa2C_bind" id="CaRyRCaMCa2C_bind">
     
     <Reactant specieID="CaRyRCaMCa2C" />
     <Reactant specieID="Ca" n="2" />
     <Product specieID="CaRyRCaMCa4" />
     <forwardRate>0.1e-2</forwardRate>
     <reverseRate>1000e-3</reverseRate>
     <Q10>0.2</Q10>
   </Reaction>
  
</ReactionScheme>
